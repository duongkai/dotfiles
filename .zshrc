# disable global search for zsh to speed up everything
unsetopt PATH_DIRS
alias ll='ls -lAhG'
alias irb='irb --simple-prompt'
alias rm='rm -i'
alias rsync='rsync --progress -rtpv'
alias grep='grep --color=auto'
alias wget='wget -c'
alias ip='ipconfig getifaddr'
alias ccze='ccze -A'
alias less='less -NRC'
